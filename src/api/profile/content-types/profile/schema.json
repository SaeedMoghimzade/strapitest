{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "profile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "biginteger"
    },
    "name": {
      "type": "string"
    },
    "password": {
      "type": "password"
    },
    "profile": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "bio": {
      "type": "blocks"
    },
    "scores": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::score.score",
      "mappedBy": "profile"
    },
    "requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::request.request",
      "mappedBy": "profile"
    },
    "evaluations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::evaluation.evaluation",
      "mappedBy": "profile"
    }
  }
}
